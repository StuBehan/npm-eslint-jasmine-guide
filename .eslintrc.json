{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true, 
        "jasmine": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "semi-style": ["error", "last"]
    }, 
    "plugins": [
      "jasmine"
    ]
}
